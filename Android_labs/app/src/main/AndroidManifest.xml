<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zoyberlo.project">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.FLASHLIGHT" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.zoyberlo.project.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.zoyberlo.project.Lab2Activity"
            android:label="@string/title_activity_lab2"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.zoyberlo.project.Lab2ContentActivity" />
        <activity android:name="com.zoyberlo.project.Lab2Present" />
        <activity android:name="com.zoyberlo.project.Lab3Activity" />
        <activity android:name="com.zoyberlo.project.Lab4Activity" />
        <activity android:name="com.zoyberlo.project.Lab5Activity" />
        <activity android:name="com.zoyberlo.project.Lab6Activity"></activity>
    </application>

</manifest>